<section class="container back">
  <h1>User Profile</h1>
  <p>
    <strong>First name:</strong><br>
    <%= @user.first_name %>
  </p>

  <p>
    <strong>Last name:</strong><br>
    <%= @user.last_name %>
  </p>
  <p>
    <strong>E-Mail address:</strong><br>
    <%= @user.email %>
  </p>

  <%= link_to 'Edit', edit_user_path(@user) %> |
  <%= link_to 'Back', users_path %>

</section>
